<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SpotifyBPM.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SpotifyBPM"
    xmlns:pages="clr-namespace:SpotifyBPM.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="SpotifyBPM">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate pages:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="App"
        ContentTemplate="{DataTemplate pages:AppPage}"
        Route="AppPage" />
</Shell>
